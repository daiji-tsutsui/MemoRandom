<% provide :title, "Log in" %>


<div class="row">
  <div class="col-lg-2"></div>

  <div class="col-lg-8">
    <%= render 'login_form', user: @user %>

    <div class="row">
      <div class="col-md-2">
        <%= link_to 'Back', top_path %>
      </div>
      <div class="col-md-6"></div>
      <div class="col-md-4 text-right">
        <s>
        If you have no account,
        <%#= link_to 'Sign up', new_user_path %>
        Sign up
        </s>
      </div>
    </div>
  </div>

  <div class="col-lg-2"></div>
</div>
